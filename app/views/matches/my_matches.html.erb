<h2>Matches created</h2>
<ul>
  <% @matches_created.each do |match| %>
    <li>
      <%= match.field.name %>
      <%= match.home_team.team_name %>
    </li>
  <% end %>
</ul>

<h2>Matches accepted</h2>
<% @matches_accepted.each do |match| %>
    <li>
      <%= match.field.name %>
      <%= match.home_team.team_name %>
    </li>
  <% end %>
