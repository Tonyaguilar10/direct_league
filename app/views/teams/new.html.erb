<h1>Teams#new</h1>
<p>Find me in app/views/teams/new.html.erb</p>
<div class="container">
  <div class="forms">
    <%= simple_form_for(@team) do |f| %>
      <%= f.input :team_name %>
      <%= f.country_select :country%>
      <%= f.input :city %>
      <%= f.input :zip_code %>
      <%= f.input :size %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
